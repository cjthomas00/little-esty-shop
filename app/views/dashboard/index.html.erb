<div class="links">
  <ul>
    <h1><%= @merchant.name %>'s Dashbaord</h1>
    <%= button_to "My Items", "/merchant/#{@merchant.id}/items", method: :get%>
    <%= button_to "My Invoices", "/merchant/#{@merchant.id}/invoices", method: :get%>
  </ul>
</div>

<section>
  <div class = "top_5_customers">
  <h2>My Favorite Customers</h2>
    <ul>
    <%@merchant.top_5_customers_by_transaction.each do |customer|%>
    <li><%= customer.first_name %> <% ' ' %> <%= customer.last_name %> <% ' ' %> <%= @merchant.successful_customer_transactions(customer.id) %></li>
     <%end%>
    </ul>
  </div>
</section>